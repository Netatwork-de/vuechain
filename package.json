{
	"name": "vuechain",
	"version": "1.0.16",
	"description": "An opinionated full stack build chain for large scale vue 2 projects",
	"publishConfig": {
		"registry": "http://localhost:4873/"
	},
	"main": "./runtime/index.js",
	"bin": "./tooling/index.js",
	"types": "./runtime/index.d.ts",
	"scripts": {
		"compile:runtime": "tsc -p tsconfig-runtime.json",
		"compile:runtime:watch": "tsc -p tsconfig-runtime.json -w",
		"compile:tooling": "tsc -p tsconfig-tooling.json",
		"compile:tooling:watch": "tsc -p tsconfig-tooling.json -w",
		"compile": "concurrently npm:compile:runtime npm:compile:tooling",
		"compile:watch": "concurrently npm:compile:runtime:watch npm:compile:tooling:watch",
		"clean": "rimraf ./runtime/**/* ./tooling/**/*",
		"prepack": "npm run clean && npm run compile"
	},
	"dependencies": {
		"@phylum/command": "^1.2.0",
		"@vue/component-compiler-utils": "^3.0.0",
		"ansi-colors": "^4.1.1",
		"chokidar": "^3.2.1",
		"css-loader": "^3.2.0",
		"fs-extra": "^8.1.0",
		"gulp-sass": "^4.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"hash-sum": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"merge-stream": "^2.0.0",
		"parse5": "^5.1.0",
		"sass": "^1.23.0",
		"sass-loader": "^8.0.0",
		"ts-loader": "^6.2.0",
		"typescript": "^3.6.4",
		"v8-compile-cache": "^2.1.0",
		"vinyl": "^2.2.0",
		"vinyl-fs": "^3.0.3",
		"vue": "^2.6.10",
		"vue-loader": "^15.7.1",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.6.10",
		"webpack": "^4.41.0",
		"webpack-dev-server": "^3.8.2"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.0",
		"@types/gulp-sass": "^4.0.0",
		"@types/gulp-sourcemaps": "0.0.32",
		"@types/hash-sum": "^1.0.0",
		"@types/merge-stream": "^1.1.2",
		"@types/node": "^10.14.21",
		"@types/parse5": "^5.0.2",
		"@types/sass": "^1.16.0",
		"@types/vinyl-fs": "^2.4.11",
		"concurrently": "^5.0.0",
		"rimraf": "^3.0.0"
	},
	"author": "Net at Work GmbH",
	"license": "Apache-2.0"
}
